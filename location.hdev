</ic>
<oc>
<par name="hasElement" base_type="ctrl" dimension="0"/>
</oc>
</interface>
<body>
<l>for i := 0 to |listOfString|-1 by 1</l>
<l>    if(listOfString[i] == targetString)</l>
<l>        hasElement := 'true'</l>
<l>        return ()</l>
<l>    endif</l>
<l>endfor</l>
<c></c>
