<body>
<c></c>
<l>MultipleLines := 'false'</l>
<l>if (|row| &gt; 1)</l>
<l>    MultipleLines := 'true'</l>
<l>endif</l>
<c></c>
<l>EdgesX := []</l>
<l>EdgesY := []</l>
