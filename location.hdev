<parameter id="ColBegin"/>
<parameter id="ColEnd"/>
<parameter id="EdgesOnly">
<default_value>'false'</default_value>
<description lang="zh_CN">If true return edges only instead of end points</description>
<sem_type>string</sem_type>
</parameter>
